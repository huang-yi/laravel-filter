{
    "name": "huang-yi/laravel-filter",
    "description": "Query filter for Laravel",
    "keywords": ["laravel", "filter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Huang Yi",
            "email": "coodeer@163.com"
        }
    ],
    "require": {
        "illuminate/database": "~5.5",
        "illuminate/config": "~5.5",
        "illuminate/console": "~5.5",
        "illuminate/filesystem": "~5.5",
        "illuminate/http": "~5.5",
        "illuminate/log": "~5.5",
        "illuminate/support": "~5.5",
        "illuminate/validation": "~5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.2",
        "mockery/mockery": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "HuangYi\\Filter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HuangYi\\Filter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HuangYi\\Filter\\FilterServiceProvider"
            ],
            "aliases": {
                "Filter": "HuangYi\\Filter\\Facades\\Filter"
            }
        }
    }
}
